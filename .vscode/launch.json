{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run infer (gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/infer",
            "args": [],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "ORT_DIR",
                    "value": "${env:ORT_DIR}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ],
            "preLaunchTask": "build-infer"
        }
    ]
}